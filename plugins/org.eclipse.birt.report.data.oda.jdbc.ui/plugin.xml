<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.birt.report.data.oda.jdbc.ui"
   name="Jdbc Plug-in"
   version="1.0.0"
   provider-name=""
   class="org.eclipse.birt.report.data.oda.jdbc.ui.JdbcPlugin">

   <runtime>
      <library name="jdbc.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.birt.report.model"/>
      <import plugin="org.eclipse.birt.data"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.draw2d"/>
      <import plugin="org.eclipse.birt.report.designer.core"/>
      <import plugin="org.eclipse.birt.report.designer.ui"/>
      <import plugin="org.eclipse.gef"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.compare"/>
   </requires>
   <extension
         point="org.eclipse.birt.report.designer.ui.datasource">
      <data-source
            type="jdbc"
            name="%datasource.name"
            id="org.eclipse.birt.data.jdbc">
         <new-data-source-wizard class="org.eclipse.birt.report.data.oda.jdbc.ui.wizards.JdbcDataSourceWizard"/>
         <data-set
               type="JdbcSelectDataSet"
               name="%dataset.jdbcselect"
               id="org.eclipse.birt.report.data.oda.jdbc.ui.JDBCSelectDataSet">
            <new-data-set-wizard class="org.eclipse.birt.report.data.oda.jdbc.ui.wizards.JdbcDataSetWizard"/>
            <data-set-editor-page
                  class="org.eclipse.birt.report.data.oda.jdbc.ui.editors.SQLDataSetEditorPage"
                  path="/"
                  name="%dataset.editor.page.query"
                  id="org.eclipse.birt.report.data.oda.jdbc.ui.data-set-editor-page1"/>
         </data-set>
         <data-source-editor-page
               class="org.eclipse.birt.report.data.oda.jdbc.ui.wizards.JdbcDataSourcePage"
               path="/"
               name="%datasource.editor.page.connection"
               id="org.eclipse.birt.report.data.oda.jdbc.ui.data-source-editor.connection"/>
         <data-source-editor-page
               class="org.eclipse.birt.report.data.oda.jdbc.ui.wizards.ExternalEditorConfigurationWizardPage"
               name="External Editor Configuration"
               id="org.eclipse.birt.report.data.oda.jdbc.ui.data-source-editor-page1"/>
      </data-source>
    </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.eclipse.birt.report.data.oda.jdbc.ui.preference.ExternalEditorPreferencePage"
            category="birt"
            name="%preference.externalEditor.title"
            id="org.eclipse.birt.data.oda.jdbc.externalEditor"/>
   </extension>
   </plugin>
