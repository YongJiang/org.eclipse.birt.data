<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.birt.data"
   name="BIRT Data Engine"
   version="1.0.0"
   provider-name="Eclipse.org">
   <runtime>
      <library name="lib/js.jar">
         <export name="*"/>
      </library>
      <library name="lib/castor-0.9.5.4-xml.jar">
         <export name="*"/>
      </library>
      <library name="lib/xercesImpl.jar">
         <export name="*"/>
      </library>
      <library name="lib/xml-apis.jar">
         <export name="*"/>
      </library>
      <library name="dte.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.birt.core"/>      
   </requires>
   <extension-point id="oda" name="BIRT Open Data Access" schema="schema/oda.exsd"/>

</plugin>
