<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.birt.report.data.oda.sampledb.ui"
   name="BIRT Sample database Ui Plug-in"
   version="1.0.0"
   provider-name="Eclipse.org"
   class="org.eclipse.birt.report.data.oda.sampledb.ui.plugin.SampledbUIPlugin">

   <runtime>
      <library name="sampledbui.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.birt.core"/>
      <import plugin="org.eclipse.birt.data.oda"/>
      <import plugin="org.eclipse.birt.report.model"/>
      <import plugin="org.eclipse.birt.report.designer.ui"/>
      <import plugin="org.eclipse.birt.report.data.oda.jdbc"/>
      <import plugin="org.eclipse.birt.report.data.oda.jdbc.ui"/>
      <import plugin="org.eclipse.birt.report.data.oda.sampledb"/>
   </requires>

   <extension
         point="org.eclipse.birt.report.designer.ui.odadatasource">
      
      <odaDataSourceUI id="org.eclipse.birt.report.data.oda.sampledb">
      </odaDataSourceUI>
      
      <odaDataSetUI id="org.eclipse.birt.report.data.oda.jdbc.JdbcSelectDataSet">
            <dataSetEditorPage
                  class="org.eclipse.birt.report.data.oda.sampledb.ui.SampleDataSetEditor"
                  path="/"
                  name="org.eclipse.birt.report.data.oda.sampledb.ui.data-set-editor-page"
                  displayName="%dataset.editor.page.query"/>
      </odaDataSetUI>
   </extension>
   
</plugin>
